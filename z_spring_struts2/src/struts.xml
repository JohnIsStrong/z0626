<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	
	<package name="df" extends="struts-default">
		
		<interceptors>
			<interceptor name="sec" class="interceptor.SecuInterceptor"></interceptor>
		</interceptors>
<global-results>
			<result name="login">index.jsp</result>
		</global-results>
		<action name="login" class="ua" method="login">
			<result name="success">showInfo.jsp</result>
			<result name="input">index.jsp</result>
		</action>
		<action name="listUserInfo" class="lui" method="listUserInfo">
			<result name="success">userInfo.jsp</result>
			<interceptor-ref name="sec"></interceptor-ref>
			<interceptor-ref name="defaultStack"></interceptor-ref>
		</action>
	</package>
</struts>